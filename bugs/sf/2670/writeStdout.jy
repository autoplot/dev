index= getParam('index',5,'which fib number')

def fib(i):
    # iterative; works fine for reasonably large i
    if i < 0:
        raise Exception("i must be >= 0")
    a = 0  # F(0)
    b = 1  # F(1)
    # after k iterations, 'a' is F(k)
    for k in range(i):
        a, b = b, a + b
    return a

f = fib(index)

print 'The %d-th Fibonacci number is %d.' % ( index, f )
